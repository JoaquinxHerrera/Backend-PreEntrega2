<div class="login"> 
    <h1>Login</h1>
    <form>
        <label for="email">Email</label>
        <input type="email" name="email" placeholder="Email" autocomplete="on"><br>
        <label for="password">Password</label>
        <input type="password" name="password" placeholder="Password" autocomplete="on">
        <button type="submit">Login</button>
    </form>
    <p>Forgot your password? Click <a href="/resetpassword">here</a></p>
    <p>Do you prefer to login with GitHub? Click <a href="/githublogin">here</a></p>
</div>  

<script src="/static/js/login.js"></script>